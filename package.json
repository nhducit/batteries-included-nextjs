{
  "name": "with-tailwindcss",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "PORT=7007 node server.js",
    "build-nextjs": "yarn build:bsb && USE_PREACT=true next build",
    "export": "next export",
    "build": "yarn build-nextjs",
    "serve": "serve -p 7777 out",
    "build-without-preact": "yarn build:bsb && next build",
    "start": "PORT=7007 USE_PREACT=true next start",
    "test": "jest",
    "format": "prettier --write 'src/**/*.{ts,tsx,js}' --config .prettierrc",
    "lint": "tsc -p tsconfig.json --noEmit",
    "lint-bk": "tsc -p tsconfig.json --noEmit && eslint --ext js,ts,tsx src",
    "analyze": "ANALYZE=true yarn build",
    "build:bsb": "bsb -make-world -clean-world && yarn format",
    "watch:bsb": "bsb -make-world -clean-world -w",
    "clean:bsb": "bsb -clean-world"
  },
  "dependencies": {
    "fp-ts": "2.9.0",
    "gentype": "3.40.0",
    "io-ts": "2.2.13",
    "io-ts-reporters": "1.2.2",
    "js-cookie": "2.2.1",
    "next": "10.0.3",
    "next-seo": "4.17.0",
    "preact": "10.5.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-react": "1.2.0",
    "reason-react": "0.9.1",
    "swr": "0.3.9"
  },
  "devDependencies": {
    "@glennsl/bs-json": "5.0.2",
    "@next/bundle-analyzer": "10.0.3",
    "@sentry/integrations": "5.27.6",
    "@sentry/node": "5.27.6",
    "@testing-library/react": "11.2.2",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.9",
    "@types/react": "17.0.0",
    "@zeit/next-source-maps": "0.0.3",
    "autoprefixer": "^10.0.2",
    "babel-jest": "26.6.3",
    "babel-plugin-inline-react-svg": "1.1.2",
    "bs-platform": "8.3.3",
    "eslint": "7.14.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.2",
    "module-alias": "2.2.2",
    "next-transpile-modules": "4.1.0",
    "organize-imports-cli": "0.8.0",
    "postcss": "^8.1.10",
    "postcss-preset-env": "6.7.0",
    "preact-render-to-string": "5.1.12",
    "prettier": "2.2.0",
    "serve": "11.3.2",
    "source-map-explorer": "2.5.0",
    "tailwindcss": "^2.0.1",
    "typescript": "4.1.2"
  },
  "lint-staged": {
    "*.{js,json,md,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "organize-imports-cli",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "resolutions": {}
}
