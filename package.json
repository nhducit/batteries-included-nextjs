{
  "name": "with-tailwindcss",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "PORT=7007 node server.js",
    "build-nextjs": "yarn build:bsb && USE_PREACT=true next build",
    "export": "next export",
    "build": "yarn build-nextjs",
    "serve": "serve -p 7777 out",
    "build-without-preact": "yarn build:bsb && next build",
    "start": "PORT=7007 USE_PREACT=true next start",
    "test": "jest",
    "format": "prettier --write 'src/**/*.{ts,tsx,js}' --config .prettierrc",
    "lint": "tsc -p tsconfig.json --noEmit",
    "lint-bk": "tsc -p tsconfig.json --noEmit && eslint --ext js,ts,tsx src",
    "analyze": "ANALYZE=true yarn build",
    "build:bsb": "bsb -make-world -clean-world && yarn format",
    "watch:bsb": "bsb -make-world -clean-world -w",
    "clean:bsb": "bsb -clean-world"
  },
  "dependencies": {
    "@next/bundle-analyzer": "9.4.4",
    "fp-ts": "2.6.5",
    "gentype": "3.25.0",
    "io-ts": "2.2.4",
    "io-ts-reporters": "1.1.0",
    "js-cookie": "2.2.1",
    "module-alias": "2.2.2",
    "next": "9.4.4",
    "next-seo": "4.5.0",
    "next-transpile-modules": "3.3.0",
    "preact": "10.4.4",
    "preact-render-to-string": "5.1.9",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-react": "1.2.0",
    "reason-react": "0.9.1",
    "swr": "0.2.2"
  },
  "devDependencies": {
    "@glennsl/bs-json": "5.0.2",
    "@sentry/integrations": "5.17.0",
    "@sentry/node": "5.17.0",
    "@testing-library/react": "10.2.1",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@zeit/next-source-maps": "0.0.3",
    "babel-jest": "26.0.1",
    "babel-plugin-inline-react-svg": "1.1.1",
    "bs-platform": "7.3.2",
    "eslint": "7.2.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.9",
    "organize-imports-cli": "0.7.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "serve": "11.3.2",
    "source-map-explorer": "2.4.2",
    "tailwindcss": "1.4.6",
    "typescript": "3.9.5"
  },
  "lint-staged": {
    "*.{js,json,md,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "organize-imports-cli",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "resolutions": {}
}
