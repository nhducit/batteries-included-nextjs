{
  "name": "with-tailwindcss",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "PORT=7007 node server.js",
    "build-nextjs": "yarn build:bsb && USE_PREACT=true next build",
    "export": "next export",
    "build": "yarn build-nextjs",
    "serve": "serve -p 7777 out",
    "build-without-preact": "yarn build:bsb && next build",
    "start": "PORT=7007 USE_PREACT=true next start",
    "test": "jest",
    "format": "prettier --write 'src/**/*.{ts,tsx,js}' --config .prettierrc",
    "lint": "tsc -p tsconfig.json --noEmit",
    "lint-bk": "tsc -p tsconfig.json --noEmit && eslint --ext js,ts,tsx src",
    "analyze": "ANALYZE=true yarn build",
    "build:bsb": "bsb -make-world -clean-world && yarn format",
    "watch:bsb": "bsb -make-world -clean-world -w",
    "clean:bsb": "bsb -clean-world",
    "prepare": "husky install"
  },
  "dependencies": {
    "@rescript/react": "0.10.3",
    "fp-ts": "2.11.1",
    "gentype": "4.1.0",
    "io-ts": "2.2.16",
    "io-ts-reporters": "2.0.0",
    "js-cookie": "3.0.0",
    "next": "12.1.0",
    "next-seo": "4.26.0",
    "preact": "10.5.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.21.0",
    "rescript": "9.1.4"
  },
  "devDependencies": {
    "@glennsl/bs-json": "5.0.4",
    "@next/bundle-analyzer": "11.1.0",
    "@sentry/integrations": "6.11.0",
    "@sentry/node": "6.11.0",
    "@testing-library/react": "12.0.0",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.1",
    "@types/react": "17.0.19",
    "@zeit/next-source-maps": "0.0.3",
    "autoprefixer": "^10.3.2",
    "bs-platform": "9.0.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "module-alias": "2.2.2",
    "next-transpile-modules": "8.0.0",
    "organize-imports-cli": "0.8.0",
    "postcss": "^8.3.6",
    "postcss-preset-env": "6.7.0",
    "preact-render-to-string": "5.1.19",
    "prettier": "2.3.2",
    "serve": "12.0.0",
    "source-map-explorer": "2.5.2",
    "tailwindcss": "^2.2.7",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{js,json,md,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "organize-imports-cli",
      "prettier --write"
    ]
  },
  "resolutions": {}
}
